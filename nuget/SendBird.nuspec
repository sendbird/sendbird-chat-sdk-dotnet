<?xml version="1.0"?>
<package >
  <metadata>
    <id>Xam.Plugin.SendBird</id>
    <version>1.0.1</version>
    <title>SendBird Plugin for Xamarin</title>
    <authors>Wilson Vargas</authors>
    <owners>Wilson Vargas</owners>
    <licenseUrl>https://github.com/smilefam/SendBird-SDK-dotNET/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://sendbird.com/</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/wilsonvargas/SendBird-SDK-dotNET/master/icon/icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
