     <!--Xamarin.Android-->
     <file src="..\src\SendBird.dll" target="lib\MonoAndroid10" />
     <file src="..\src\websocket-sharp.dll" target="lib\MonoAndroid10" />

     <!--Xamarin.iOS-->
     <file src="..\src\SendBird.dll" target="lib\Xamarin.iOS10" />
     <file src="..\src\websocket-sharp.dll" target="lib\Xamarin.iOS10" />

     <!--UWP-->
     <file src="..\src\SendBird.dll" target="lib\UAP10" />
